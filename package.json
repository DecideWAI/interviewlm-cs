{
  "name": "interviewlm-cs",
  "version": "0.1.0",
  "private": true,
  "overrides": {
    "@auth/core": "^0.40.0"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:api": "jest __tests__/api",
    "test:components": "jest __tests__/components",
    "test:integration": "jest __tests__/integration",
    "test:integration:docker": "jest __tests__/integration/database --runInBand --forceExit",
    "test:integration:watch": "jest __tests__/integration/database --watch --runInBand",
    "db:seed": "npx prisma db seed",
    "docker:dev": "./scripts/docker-dev.sh start",
    "docker:dev:stop": "./scripts/docker-dev.sh stop",
    "docker:dev:logs": "./scripts/docker-dev.sh logs",
    "docker:test": "./scripts/docker-test.sh run",
    "docker:test:watch": "./scripts/docker-test.sh interactive",
    "docker:build": "docker build -t interviewlm-app .",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down"
  },
